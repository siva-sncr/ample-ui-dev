{
  "name": "ample-manage",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod": "ng build --prod --aot",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "e2e": "protractor",
    "rimraf": "rimraf",
    "clean:dist": "npm run rimraf -- dist",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:awcache": "npm run rimraf -- .awcache",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:dev": "npm run clean:awcache && webpack-dev-server --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress",
    "build:prod": "npm run clean:dist && npm run clean:aot && cross-env BUILD_AOT=1 webpack --config config/webpack.prod.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngx-translate/core": "^7.1.0",
    "angular2-datatable": "^0.6.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "js-yaml": "^3.9.1",
    "ng2-table": "^1.3.2",
    "rxjs": "^5.4.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@angularclass/hmr-loader": "^3.0.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.3",
    "codelyzer": "~3.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "html-loader": "^0.5.1",
    "install": "^0.10.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-router-loader": "^2.1.0",
    "ngc-webpack": "^3.2.2",
    "npm": "^5.3.0",
    "optimize-js-plugin": "0.0.4",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.1",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "to-string-loader": "^1.1.5",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "webpack": "^3.5.4"
  }
}
